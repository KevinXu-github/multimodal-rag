# LLM API Keys (OPTIONAL - Ollama is default and needs no key)
# Only needed if you want to use cloud LLMs instead of Ollama
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password123

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=

# Application Settings
LOG_LEVEL=INFO
MAX_WORKERS=4
CHUNK_SIZE=512
CHUNK_OVERLAP=50

# Model Configuration
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
LLM_PROVIDER=ollama
LLM_MODEL=llama3.2
LLM_TEMPERATURE=0.0
MAX_TOKENS=2000

# Evaluation
ENABLE_EVALUATION=true
EVAL_OUTPUT_DIR=./logs/eval
